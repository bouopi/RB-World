local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("RB World Killer", "Midnight")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Main Stuff")
Section:NewKeybind("Aimbot", "Works Best Standing Still/End of Combos", Enum.KeyCode.R, function()
loadstring(game:HttpGet('https://pastebin.com/raw/mvHqL6sS'))()
end)
Section:NewKeybind("Dribble Combo(Advance)", "Works Best Standing Still/End of Combos", Enum.KeyCode.Q, function()
loadstring(game:HttpGet('https://pastebin.com/raw/ngeGQzAF'))()
end)
local Tab = Window:NewTab("Player")
local Section = Tab:NewSection("Misc")
Section:NewButton("Speed(Rshift)", "Be Careful Better if you Tap", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/cbxrBbLw'))()
end)
local Tab = Window:NewTab("Credits")
local Section = Tab:NewSection("Discord:GooseyYT#0001")
local Section = Tab:NewSection("Roblox User:ShippingPowr")
local Section = Tab:NewSection("Made Fresh ;)")
Section:NewKeybind("Close Ui", "Simple", Enum.KeyCode.RightShift, function()
	Library:ToggleUI()
end)
s
